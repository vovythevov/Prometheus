#
# This module is provided as @APPLICATION_NAME@_USE_FILE by @APPLICATION_NAME@Config.cmake.
# It can be INCLUDED in a project to load the needed compiler and linker
# settings to use @APPLICATION_NAME@.
#

if(NOT @APPLICATION_NAME@_USE_FILE_INCLUDED)
  set(@APPLICATION_NAME@_USE_FILE_INCLUDED 1)

  set(CMAKE_MODULE_PATH
    ${CMAKE_MODULE_PATH}
    ${@APPLICATION_NAME@_CMAKE_DIRS})
  # Add include directories needed to use ${APPLICATION_NAME}.
  include_directories(${@APPLICATION_NAME@_INCLUDE_DIRS})

  # Add link directories needed to use @APPLICATION_NAME@.
  link_directories(${@APPLICATION_NAME@_LIBRARY_DIRS} ${@APPLICATION_NAME@_EXTERNAL_LIBRARY_DIRS})

  if (NOT DEFINED QT_QMAKE_EXECUTABLE AND DEFINED @APPLICATION_NAME@_QT_QMAKE_EXECUTABLE)
    set(QT_QMAKE_EXECUTABLE ${@APPLICATION_NAME@_QT_QMAKE_EXECUTABLE})
  endif()

  set(@APPLICATION_NAME@_BUILD_TESTING ${BUILD_TESTING})

endif()
